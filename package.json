{
  "name": "wasm-prototyping",
  "version": "1.0.0",
  "description": "WebAssembly prototyping",
  "main": "index.js",
  "scripts": {
    "build": "emcc src/hello.c -I src -Os -s WASM=1 -o library.wasm",
    "cbuild": "rm -Rf build && npm run set-emscripten-path && npm run cmake-build && npm run make",
    "cmake-build": "cmake -H. -B./build -DCMAKE_VERBOSE_MAKEFILE:BOOL=ON -DCMAKE_BUILD_TYPE=Release -DCMAKE_TOOLCHAIN_FILE=${EMSDK}/cmake/Modules/Platform/Emscripten.cmake",
    "make": "cd build && make",
    "test": "echo \"Error: no test specified\" && exit 1",
    "http-server": "http-server",
    "serve-build": "npm run http-server",
    "set-emscripten-path": "export EMSDK=/home/jojanper/bin/emsdk/emscripten/1.38.29"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jojanper/wasm-prototyping.git"
  },
  "keywords": [
    "WebAssembly",
    "WASM",
    "emscripten",
    "C",
    "C++"
  ],
  "author": "Juha Ojanper√§ (juha.ojanpera@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jojanper/wasm-prototyping/issues"
  },
  "homepage": "https://github.com/jojanper/wasm-prototyping#readme",
  "devDependencies": {
    "http-server": "latest"
  }
}
